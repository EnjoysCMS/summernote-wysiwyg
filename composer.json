{
  "name": "enjoyscms/summernote",
  "description": "Wysiwyg Summernote for EnjoysCMS",
  "type": "enjoyscms-wysiwyg",
  "require": {
    "php": ">=8.0",
    "enjoyscms/core": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "EnjoysCMS\\WYSIWYG\\Summernote\\": "src/"
    }
  },
  "extra": {
    "module-name": "Summernote WYSIWYG Editor",
    "use-migrations": false
  },
  "scripts": {
    "assets-install": "EnjoysCMS\\WYSIWYG\\Summernote\\Composer::assetsInstall",
    "post-install-cmd": "@assets-install",
    "post-update-cmd": "@assets-install"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
